                   greater.docs = [doc for doc in greater.docs if doc._id not in ids].concat(less.docs)
                   return greater
               ))
           )
        elif arg[1] == 'like':
           #selectors[index][arg[0]]['$gte'] = arg[2];
           #promises.push(db.search({query: arg[2], 'fields': [arg[0]], include_docs: False}));
